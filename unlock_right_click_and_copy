javascript:(function(){function desbloquearCopiaEClickDireito(){document.oncopy=null;document.oncut=null;document.body.oncopy=null;document.body.oncut=null;document.addEventListener('copy',function(event){event.stopPropagation();},true);document.addEventListener('cut',function(event){event.stopPropagation();},true);var elements=document.querySelectorAll('*');elements.forEach(function(el){el.style.userSelect='text';el.style.webkitUserSelect='text';el.style.MozUserSelect='text';el.style.msUserSelect='text';});document.oncontextmenu=null;document.body.oncontextmenu=null;document.addEventListener('contextmenu',function(event){event.stopPropagation();},true);console.log('Funções de copiar e botão direito do mouse desbloqueadas.');}desbloquearCopiaEClickDireito();})();